{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://gh-proxy.com/https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "direct",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": false
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "proxyDns",
        "address": "tls://8.8.8.8",
        "detour": "Proxy"
      },
      {
        "tag": "localDns",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "localDns"
      },
      {
        "rule_set": "geosite-cn",
        "server": "localDns"
      },
      {
        "clash_mode": "direct",
        "server": "localDns"
      },
      {
        "clash_mode": "global",
        "server": "proxyDns"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "server": "proxyDns"
      }
    ],
    "final": "localDns",
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.0/30"
      ],
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 2080
    }
  ],
  "outbounds": [
    {
      "tag": "Proxy",
      "type": "selector",
      "outbounds": [
        "auto",
        "direct",
        "{all}"
      ]
    },
    {
      "tag": "Telegram",
      "type": "selector",
      "outbounds": [
        "Proxy",
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "Korea",
        "America",
        "Others"
      ]
    },
    {
      "tag": "Spotify",
      "type": "selector",
      "outbounds": [
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "Korea",
        "America",
        "Proxy",
        "Others"
      ],
      "default": "HongKong"
    },
    {
      "tag": "TikTok",
      "type": "selector",
      "outbounds": [
        "Proxy",
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "Korea",
        "America"
      ],
      "default": "Japan"
    },
    {
      "tag": "AI",
      "type": "selector",
      "outbounds": [
        "Proxy",
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "Korea",
        "America",
        "Others"
      ],
      "default": "America"
    },
    {
      "tag": "Meta",
      "type": "selector",
      "outbounds": [
        "Proxy",
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "Korea",
        "America",
        "Others"
      ]
    },
    {
      "tag": "Google",
      "type": "selector",
      "outbounds": [
        "Proxy",
        "HongKong",
        "TaiWan",
        "Singapore",
        "Japan",
        "Korea",
        "America",
        "Others"
      ]
    },
    {
      "tag": "China",
      "type": "selector",
      "outbounds": [
        "direct",
        "Proxy"
      ]
    },
    {
      "tag": "HongKong",
      "type": "selector",
      "outbounds": [
        "{all}",
        "direct"
      ]
    },
    {
      "tag": "TaiWan",
      "type": "selector",
      "outbounds": [
        "{all}",
        "direct"
      ]
    },
    {
      "tag": "Singapore",
      "type": "selector",
      "outbounds": [
        "{all}"
      ]
    },
    {
      "tag": "Japan",
      "type": "selector",
      "outbounds": [
        "{all}"
      ]
    },
    {
      "tag": "Korea",
      "type": "selector",
      "outbounds": [
        "{all}",
        "direct"
      ]
    },
    {
      "tag": "America",
      "type": "selector",
      "outbounds": [
        "{all}"
      ]
    },
    {
      "tag": "Others",
      "type": "selector",
      "outbounds": [
        "{all}"
      ]
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "Proxy",
    "rules": [
      {
        "inbound": ["tun-in", "mixed-in"],
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "clash_mode": "rule",
        "action": "reject"
      },
      {
        "rule_set": "geosite-category-ads-all",
        "clash_mode": "global",
        "outbound": "Proxy"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "Proxy"
      },
      {
        "domain": [
          "clash.razord.top",
          "yacd.metacubex.one",
          "yacd.haishan.me",
          "d.metacubex.one"
        ],
        "outbound": "direct"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-openai",
        "outbound": "AI"
      },
      {
        "rule_set": ["geosite-youtube"],
        "outbound": "Youtube"
      },
      {
        "rule_set": ["geoip-google", "geosite-google", "geosite-github"],
        "outbound": "Google"
      },
      {
        "rule_set": ["geoip-telegram", "geosite-telegram"],
        "outbound": "Telegram"
      },
      {
        "rule_set": ["geoip-twitter", "geosite-twitter"],
        "outbound": "Twitter"
      },
      {
        "rule_set": ["geoip-facebook", "geosite-facebook"],
        "outbound": "Facebook"
      },
      {
        "rule_set": "geosite-bilibili",
        "outbound": "China"
      },
      {
        "rule_set": "geosite-bahamut",
        "outbound": "Proxy"
      },
      {
        "rule_set": "geosite-spotify",
        "outbound": "Spotify"
      },
      {
        "rule_set": "geosite-tiktok",
        "outbound": "TikTok"
      },
      {
        "rule_set": ["geoip-netflix", "geosite-netflix"],
        "outbound": "Proxy"
      },
      {
        "rule_set": "geosite-disney",
        "outbound": "Proxy"
      },
      {
        "rule_set": ["geoip-apple", "geosite-apple", "geosite-amazon"],
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-microsoft",
        "outbound": "Proxy"
      },
      {
        "rule_set": ["geosite-category-games", "geosite-dmm", "geosite-steam"],
        "outbound": "Proxy"
      },
      {
        "rule_set": ["geosite-hbo", "geosite-primevideo"],
        "outbound": "Proxy"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "outbound": "China"
      },
      {
        "rule_set": ["geoip-cn", "geosite-cn"],
        "outbound": "China"
      }
    ]
  }
}
